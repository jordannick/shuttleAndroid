<?xml version="1.0" encoding="utf-8"?>

<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item
        android:bottom="0dp"
        android:left="-2dp"
        android:right="-2dp"
        android:top="-2dp">
        <shape android:shape="rectangle">
            <stroke
                android:width="2dp"
                android:color="@color/OSU_orange"/>

            <solid android:color="#FF191b1b"/>

            <padding
                android:bottom="10dp"
                android:right="10dp"
                android:top="10dp"/>
        </shape>
    </item>

</layer-list>